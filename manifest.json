{
  "name": "MeiLuft First-Person-View Umgebungs-App",
  "short_name": "FPV-App",
  "description": "Eine App von MeiLuft zur Darstellung der Umgebung in First-Person-View basierend auf GPS-Daten und OpenStreetMap",
  "start_url": "/index.html",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#4285f4",
  "icons": [
    {
      "src": "icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "orientation": "portrait",
  "permissions": [
    "geolocation",
    "deviceorientation"
  ]
}
